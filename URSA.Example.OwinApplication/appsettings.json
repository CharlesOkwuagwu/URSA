{
    "ursa": {
        "core": {
            "installerAssemblyNameMask": "URSA*",
            "serviceProviderTypeName": "URSA.ComponentModel.AutoFacComponentProvider, URSA.AutoFac",
            "converterProviderTypeName": "URSA.Web.Converters.DefaultConverterProvider, URSA.Core",
            "controllerActivatorTypeName": "URSA.Web.DefaultControllerActivator, URSA.Core"
        },
        "description": {
            "defaultStoreFactoryName": "in-memory",
            "typeDescriptionBuilderTypeName": "URSA.Web.Http.Description.HydraCompliantTypeDescriptionBuilder, URSA.Http.Description"
        }
    },
    "romanticWeb": {
        "factories": [
            {
                "name": "in-memory",
                "threadSafe": true,
                "metaGraphUri": "meta://ursa/",
                "ontologies": [
                    {
                        "prefix": "hydra",
                        "uri": "http://www.w3.org/ns/hydra/core#"
                    },
                    {
                        "prefix": "shacl",
                        "uri": "http://www.w3.org/ns/shacl#"
                    },
                    {
                        "prefix": "guid",
                        "uri": "http://openguid.net/rdf#"
                    },
                    {
                        "prefix": "ursa",
                        "uri": "http://alien-mcl.github.io/URSA/vocabulary#"
                    },
                    {
                        "prefix": "odata",
                        "uri": "http://docs.oasis-open.org/odata/odata/v4.0/"
                    }
                ],
                "mappingAssemblies": [
                    { "assembly": "URSA.Http.Description" },
                    { "assembly": "URSA.Example.OwinApplication" }
                ]
            },
            {
                "name": "file",
                "metaGraphUri": "meta://ursa/",
                "ontologies": [
                    {
                        "prefix": "hydra",
                        "uri": "http://www.w3.org/ns/hydra/core#"
                    },
                    {
                        "prefix": "shacl",
                        "uri": "http://www.w3.org/ns/shacl#"
                    },
                    {
                        "prefix": "guid",
                        "uri": "http://openguid.net/rdf#"
                    },
                    {
                        "prefix": "ursa",
                        "uri": "http://alien-mcl.github.io/URSA/vocabulary#"
                    },
                    {
                        "prefix": "odata",
                        "uri": "http://docs.oasis-open.org/odata/odata/v4.0/"
                    }
                ],
                "mappingAssemblies": [
                    { "assembly": "URSA.Http.Description" },
                    { "assembly": "URSA.Example.OwinApplication" }
                ]
            }
        ]
    },
    "romanticWeb.dotNetRDF": {
        "stores": {
            "file": [
                {
                    "name": "file", 
                    "filePath": "~/App_Data/store.trig"
                }
            ]
        }
    }
}